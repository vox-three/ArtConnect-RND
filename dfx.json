{
    "canisters": {
        "artconnect": {
            "type": "custom",
            "main": "src/backend/index.ts",
            "candid": "src/backend/index.did",
            "build": "npx azle artconnect",
            "wasm": ".azle/artconnect/artconnect.wasm",
            "gzip": true,
            "declarations": {
                "node_compatibility": true,
                "env_override": ""
            }
        },
        "assets": {
            "dependencies": ["artconnect"],
            "frontend": {
                "entrypoint": "dist/index.html"
            },
            "source": ["src/frontend/assets", "dist/assets"],
            "type": "assets"
        }
    },
    "defaults": {
        "build": {
            "packtool": ""
        }
    },
    "networks": {
        "local": {
            "bind": "127.0.0.1:4943",
            "type": "ephemeral"
        },
        "ic": {
            "providers": ["https://mainnet.dfinity.network"],
            "type": "persistent"
        }
    },
    "version": 1
}
